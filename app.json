{
  "expo": {
    "name": "CriptoVen Trade",
    "slug": "OrbitPay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/OrbitIconApp.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/orbitInitial.png",
      "backgroundColor": "#00000000",
      "resizeMode": "cover"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CriptoVen Trade access your camera.",
          "microphonePermission": 	"Allow CriptoVen Trade to access your microphone"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.criptoventrade.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/OrbitIconApp.png",
        "backgroundColor": "#fafafa"
      },
      "package": "com.orbitpay.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ef3f6a3c-b7b5-49ea-acf1-f1fdd05f1fbd"
      }
    },
    "owner": "breaper03"
  }
}
